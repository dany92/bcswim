<h1>Manage Accounts</h1>

<div>
 <% @users.each do |user| %>
    <div class="user">
      <p><%= "First Name:  #{user.first_name}" %></p>
      <p><%= "Last Name: #{user.last_name}" %></p>
      <p><%= "Email: #{user.email}" %></p>
      <p><%= "Role: #{user.role}" %></p>
    </div>
    <% end %>
</div>
