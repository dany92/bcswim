<h3>Users</h3>

<div>
	<table class="table table-hover">
		<thead>
  		<tr>
    	<th colspan="1">First Name</th>
    	<th colspan="1">Last Name</th>
    	<th colspan="1">Email</th>
    	<th colspan="1">Role</th>
    	<th>Actions</th>
  		</tr>
	</thead>
	<tbody>
 <% @users.each do |user| %>
 <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td>Action</td>
  </tr>
    <% end %>
</tbody>
    </table>
</div>
