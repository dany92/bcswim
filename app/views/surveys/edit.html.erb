<%= form_for (@survey), :html => {:class => "form-horizontal center"} do |f| %>
<br />
<%=f.text_field :title, class: "form-control", type: "text"%> <br />
<%=f.text_field :date, class: "form-control", type: "date"%><br />

<%= f.select :category, options_for_select([ "Swim Match", "Normal Workout" ]), {:onchange => "reset_form(this.id,'page_type_msg');"},class: "form-control"  %><br />

<%= collection_select(:question, :question_id, @questions, :id, :context, {:include_hidden =>false}, {:multiple => true}) %>
<br /><br />
<%= f.submit "Save Changes", class: "btn btn-primary" %>
<%end%>
