<%= form_for (@survey) do |f| %>
<%=f.text_field :title, :placeholder => "Title"%>
<%=f.text_field :date, :placeholder => "Date"%>
<%= f.select :category, options_for_select([ "Swim Match", "Normal Workout" ]), :onchange => "reset_form(this.id,'page_type_msg');" %><br />
<%= collection_select(:question, :question_id, @questions, :id, :context) %><br />
<%= f.submit "Submit", class: "btn-submit" %>
<%end%>
